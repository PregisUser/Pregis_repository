      <query id="_00XmYFCqEe6vMq5pHR7WsA" label="Resi_Omaggi" value="SELECT &#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN,&#13;&#10;  &#9;K.FKDAT,&#13;&#10;&#9;'PREZZO' AS TIPOCONDIZIONE,&#13;&#10;&#9;K.KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KONV.KNUMV,&#13;&#10;&#9;&#9;KONV.STUNR, &#13;&#10;&#9;&#9;KONV.KPOSN,&#13;&#10;&#9;&#9;KONV.KWERT,&#13;&#10;&#9;&#9;VBRK_P.VBELN,&#13;&#10;&#9;&#9;VBRK_P.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;    KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;(&#13;&#10;&#9;&#9; SELECT &#13;&#10;&#9;&#9;&#9;VBRK.KNUMV, &#13;&#10;&#9;&#9;&#9;VBRP.POSNR,&#13;&#10;&#9;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;&#9;VBRK.FKDAT&#13;&#10;&#9;&#9; FROM &#13;&#10;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;&#9; WHERE   YEAR(VBRK.FKDAT) = 2023 AND PSTYV = 'RENN' AND&#13;&#10;&#9;&#9;&#9;FKART IN ('RE') AND FKSTO != 'X') AS VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;   &#9;&#9;&#9;&#9;&#9;&#9;&#9;KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;KOAID = 'A' AND &#13;&#10;&#9;&#9;KSCHL = 'Z100'&#13;&#10;) K" xmi:id="_00XmYVCqEe6vMq5pHR7WsA"/>
