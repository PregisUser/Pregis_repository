      <query id="_F3c6oMf4Ee2RtLsMFFY6oQ" label="RC_KONV_ScontiResi_A" value="SELECT COUNT(*)&#13;&#10;FROM (&#13;&#10;SELECT &#13;&#10;&#9;  KONV.KNUMV,&#13;&#10;&#9;  KONV.KPOSN,&#13;&#10;&#9;   VBRK_P.VBELN,&#13;&#10;     &#9;VBRK_P.FKDAT,&#13;&#10;&#9;&#9;'Sconto'  AS TIPOCONDIZIONE,&#13;&#10;&#9; &#9;SUM(KONV.KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;VBRK.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN&#13;&#10;WHERE    YEAR(FKDAT) = 2022 AND MONTH(FKDAT) IN (7,8) AND FKSTO != 'X' AND &#13;&#10;&#9;&#9;FKART = 'RE') VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KOAID = 'A'  AND&#13;&#10;&#9;KSTAT = '' and&#13;&#10; &#9;KINAK = ''&#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;     VBRK_P.FKDAT&#13;&#10;) FMM" xmi:id="_F3c6ocf4Ee2RtLsMFFY6oQ"/>
