      <query id="_QNeZYBP1Ee2XRurWHir-iw" label="KONV_FatturatoCMV" value="SELECT &#13;&#10;VBRK_P.FKDAT,&#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;    VBRK_P.VBELN,&#13;&#10;&#9;KONV.KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019  AND FKSTO != 'X'&#13;&#10;&#9;&#9;) VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE&#13;&#10;&#9;KSCHL = 'ZCST'" xmi:id="_QNeZYRP1Ee2XRurWHir-iw"/>
      <query id="_g2yVABP1Ee2XRurWHir-iw" label="KONV_FatturatoLordo_B" value="SELECT &#13;&#10;&#9;K.FKDAT,&#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN ,&#13;&#10;&#9;'Prezzo' AS TipoCondizione,&#13;&#10;&#9;KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;KONV.KNUMV, STUNR, KPOSN, KWERT, VBRK_P.VBELN, VBRK_P.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;KNUMV, POSNR,VBRK.VBELN, VBRK.FKDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;&#9;&#9;&#9;&#9;WHERE &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;YEAR(VBRK.FKDAT) = 2022 AND MONTH(VBRK.FKDAT) = 7 AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9; FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV') AND FKSTO != 'X'&#13;&#10;&#9;&#9;&#9;&#9;) VBRK_P &#13;&#10;&#9;&#9;&#9;&#9;ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;   KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B'  AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = '') K&#9;&#13;&#10;INNER JOIN &#13;&#10;&#9;(&#13;&#10;&#9;SELECT  &#13;&#10;&#9;&#9;KNUMV, KPOSN, MAX(STUNR) AS MSTUNR&#13;&#10;&#9;FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = ''&#13;&#10;&#9;GROUP BY &#13;&#10;&#9;&#9;KNUMV, KPOSN) KMAX ON K.KNUMV = KMAX.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;     &#9;&#9;&#9;&#9;&#9;&#9;&#9;        K.KPOSN = KMAX.KPOSN AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;                                    K.STUNR = KMAX.MSTUNR" xmi:id="_g2yVARP1Ee2XRurWHir-iw"/>
      <query id="_r-tEoBP1Ee2XRurWHir-iw" label="KONV_FatturatoMediaMobile" value="SELECT &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;    VBRK_P.VBELN,&#13;&#10;&#9;KONV.KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;POSNR&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN --AND VBRK.ERDAT = VBRP.ERDAT&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;VBRK.ERDAT &gt;= 2019 --ADD_MONTHS(ADD_DAYS (TO_DATE (current_date, 'YYYY-MM-DD'), -7),-1) &#13;&#10;&#9;&#9;) VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE&#13;&#10;&#9;KSCHL = 'VPRS'" xmi:id="_r-tEoRP1Ee2XRurWHir-iw"/>
      <query id="_zUF90BP1Ee2XRurWHir-iw" label="KONV_OmaggiVendite_A" value="SELECT &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;&#9;'SCONTO'  AS TIPOCONDIZIONE,&#13;&#10;&#9; SUM(KONV.KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;&#9;VBRK.VBELN&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN --AND VBRK.ERDAT = VBRP.ERDAT&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;VBRK.ERDAT &gt;= 2019  AND -- ADD_MONTHS(ADD_DAYS (TO_DATE (current_date, 'YYYY-MM-DD'), -7),-1)  AND &#13;&#10;&#9;&#9;FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV')) VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KSCHL = 'Z100' AND&#13;&#10;&#9;KOAID = 'A' &#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN" xmi:id="_zUF90RP1Ee2XRurWHir-iw"/>
      <query id="_5lUEQBP1Ee2XRurWHir-iw" label="KONV_OmaggiVendite_B" value="SELECT &#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;k.VBELN,&#13;&#10;&#9; 'PREZZO' AS TIPOCONDIZIONE,&#13;&#10;&#9;K.KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KONV.KNUMV,&#13;&#10;&#9;&#9;KONV.STUNR, &#13;&#10;&#9;&#9;KONV.KPOSN,&#13;&#10;&#9;&#9;KONV.KWERT,&#13;&#10;&#9;&#9;VBRK_P.VBELN&#13;&#10;&#9; FROM &#13;&#10;&#9;    KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;(&#13;&#10;&#9;&#9; SELECT &#13;&#10;&#9;&#9;&#9;VBRK.KNUMV, &#13;&#10;&#9;&#9;&#9;VBRP.POSNR,&#13;&#10;&#9;&#9;&#9;VBRK.VBELN&#13;&#10;&#9;&#9; FROM &#13;&#10;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN --AND VBRK.ERDAT = VBRP.ERDAT&#13;&#10;&#9;&#9; WHERE &#13;&#10;&#9;&#9;&#9;VBRK.ERDAT &gt;= 2019 AND -- ADD_MONTHS(ADD_DAYS (TO_DATE (current_date, 'YYYY-MM-DD'), -7),-1)  AND &#13;&#10;&#9;&#9;&#9;FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV')) AS VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;   &#9;&#9;&#9;&#9;&#9;&#9;&#9;KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KSCHL = 'Z100'&#13;&#10;&#9;) K&#9;&#13;&#10;INNER JOIN &#13;&#10;&#9;(&#13;&#10;&#9; SELECT  &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;KPOSN, &#13;&#10;&#9;&#9;MAX(STUNR) AS MSTUNR&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KSCHL = 'Z100'&#13;&#10;     GROUP BY &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;KPOSN) KMAX ON K.KNUMV = KMAX.KNUMV AND &#13;&#10;&#9;&#9;&#9;&#9;&#9;   K.KPOSN = KMAX.KPOSN AND &#13;&#10;&#9;&#9;&#9;&#9;&#9;   K.STUNR = KMAX.MSTUNR" xmi:id="_5lUEQRP1Ee2XRurWHir-iw"/>
