      <query id="_WEmTAN-DEe2Fs_bPy8gicg" label="ZSD_ANAL_PREZ" value="SELECT *&#13;&#10;FROM (&#13;&#10;SELECT &#13;&#10;&#9;ZSD_ANAL_PREZ.VBELN, ZSD_ANAL_PREZ.POSNR, VBRP.ERDAT,ZSD_ANAL_PREZ.PRICECOND, VBRP.VKORG,KSCHL&#13;&#10;FROM &#13;&#10;&#9;ZSD_ANAL_PREZ &#13;&#10;INNER JOIN&#13;&#10; ( &#13;&#10;&#9;SELECT AUBEL, AUPOS,ERDAT,VKORG&#13;&#10;&#9;FROM&#13;&#10;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;VBRK.ERDAT, VBRP.AUBEL, VBRP.AUPOS, VBRK.VKORG, VBRP.VBELN, VBRP.POSNR&#13;&#10;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;VBRP&#13;&#10;&#9;&#9;&#9;INNER JOIN &#13;&#10;&#9;&#9;&#9;&#9;VBRK on VBRP.VBELN = VBRK.VBELN&#13;&#10;&#9;&#9;&#9;WHERE YEAR(VBRK.ERDAT) = 2022 AND VBRK.FKSTO != 'X'&#13;&#10;&#9;&#9;)  VBRK_P &#13;&#10;&#9;&#9;&#9;INNER JOIN &#13;&#10;&#9;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;SELECT  VBELN,POSNN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;FROM VBFA&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;WHERE YEAR(ERDAT) = 2022&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;AND VBTYP_V = 'C'&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;AND VBTYP_N   = 'M'&#13;&#10;&#9;&#9;&#9;&#9;&#9;) VBFA ON &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK_P.VBELN = VBFA.VBELN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9; AND &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK_P.POSNR = VBFA.POSNN) &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;ZSD_ANAL_PREZ.VBELN = VBRP.AUBEL &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;ZSD_ANAL_PREZ.POSNR = VBRP.AUPOS&#13;&#10;&#13;&#10;&#13;&#10;) FMM&#13;&#10;WHERE (VBELN = '2108519719' AND POSNR = '000100') &#13;&#10;OR (VBELN = '2108442530' AND POSNR = '000100')&#13;&#10;OR (VBELN = '2108428629' AND POSNR = '000320')" xmi:id="_WEmTAd-DEe2Fs_bPy8gicg"/>
