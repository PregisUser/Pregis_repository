      <query id="_QrWLkBMNEe2trNvDWGyjVA" label="KONV_TEST" properties="deleted&#10;&#10;true&#10;&#10;" value="select KONV.KNUMV,STUNR, KPOSN,KWERT, KINAK, KOAID&#13;&#10;FROM KONV&#13;&#10;&#9;&#9;&#9;&#9;INNER join&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT KNUMV, POSNR&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER join&#9;&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP on&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;VBRK.VBELN = VBRP.VBELN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;WHERE KNUMV  = '0012426646'&#13;&#10;&#9;&#9;&#9;) VBRK_P ON&#13;&#10;KONV.KNUMV = VBRK_P.KNUMV&#13;&#10;AND&#13;&#10;KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE KOAID = 'B'  AND KINAK = ''&#13;&#10;ORDER BY STUNR DESC" xmi:id="_QrWLkRMNEe2trNvDWGyjVA"/>
      <query id="_W0HOsBMPEe2trNvDWGyjVA" label="KONV_FattNetto_A" properties="deleted&#10;&#10;true&#10;&#10;" value="select TOP 10 KONV.KNUMV,STUNR, KPOSN,SUM(KWERT) AS KWERT&#13;&#10;FROM KONV&#13;&#10;&#9;&#9;&#9;&#9;INNER join&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT KNUMV, POSNR&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER join&#9;&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP on&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;VBRK.VBELN = VBRP.VBELN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;WHERE FKART = 'RE'&#13;&#10;&#9;&#9;&#9;) VBRK_P ON&#13;&#10;KONV.KNUMV = VBRK_P.KNUMV&#13;&#10;AND&#13;&#10;KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE KOAID = 'A'  AND KSTAT = ''&#13;&#10;GROUP BY KONV.KNUMV,STUNR, KPOSN&#13;&#10;ORDER BY KPOSN,STUNR DESC" xmi:id="_W0HOsRMPEe2trNvDWGyjVA"/>
      <query id="_kLzEsBMTEe2trNvDWGyjVA" label="KONV_FatturatoLordo_B" value="SELECT &#13;&#10;&#9;K.FKDAT,&#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN ,&#13;&#10;&#9;'Prezzo' AS TipoCondizione,&#13;&#10;&#9;KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;KONV.KNUMV, STUNR, KPOSN, KWERT, VBRK_P.VBELN, VBRK_P.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;KNUMV, POSNR,VBRK.VBELN, VBRK.FKDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;&#9;&#9;&#9;&#9;WHERE &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019 AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9; FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV') AND FKSTO != 'X'&#13;&#10;&#9;&#9;&#9;&#9;) VBRK_P &#13;&#10;&#9;&#9;&#9;&#9;ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;   KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B'  AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = '') K&#9;&#13;&#10;INNER JOIN &#13;&#10;&#9;(&#13;&#10;&#9;SELECT  &#13;&#10;&#9;&#9;KNUMV, KPOSN, MAX(STUNR) AS MSTUNR&#13;&#10;&#9;FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = ''&#13;&#10;&#9;GROUP BY &#13;&#10;&#9;&#9;KNUMV, KPOSN) KMAX ON K.KNUMV = KMAX.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;     &#9;&#9;&#9;&#9;&#9;&#9;&#9;        K.KPOSN = KMAX.KPOSN AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;                                    K.STUNR = KMAX.MSTUNR" xmi:id="_kLzEsRMTEe2trNvDWGyjVA"/>
      <query id="_Q3SfYBMZEe2trNvDWGyjVA" label="KONV_FatturatoLordo_A" value="SELECT &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KPOSN,    &#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;    VBRK_P.FKDAT,&#13;&#10;&#9;'Sconto' AS TIPOCONDIZIONE,&#13;&#10;&#9;SUM(KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV,&#13;&#10;&#9;&#9; POSNR, &#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;VBRK.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9; WHERE &#13;&#10;&#9;YEAR(VBRK.FKDAT) &gt;= 2019 AND&#13;&#10;&#9;&#9; FKART IN (' F2', 'FX', 'ZGEP', 'ZPDV')&#13;&#10;&#9;) &#13;&#10;&#9;&#9;VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;  &#9;&#9;&#9;&#9;  KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KOAID = 'A' &#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KPOSN,    &#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;VBRK_P.FKDAT&#13;&#10;" xmi:id="_Q3SfYRMZEe2trNvDWGyjVA"/>
