      <query id="_9GoKEKuvEe2f7ew1vhc2Sg" label="KONV_CMA" value="SELECT &#13;&#10;VBRK_P.FKDAT,&#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;    VBRK_P.VBELN,&#13;&#10;&#9;KONV.KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;WHERE YEAR(FKDAT) &gt;= 2019 AND FKSTO != 'X'&#13;&#10;&#9;&#9;) VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE&#13;&#10;&#9;KSCHL = 'ZCSA'" xmi:id="_9GoKEauvEe2f7ew1vhc2Sg"/>
