      <query contextMode="true" id="_RYKY0CkHEe2lYIaDA-HbiA" label="Check_FatturatoLordo_B" value="SELECT &#13;&#10;&#9;K.ERDAT,&#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN,&#13;&#10;&#9;KOAID,&#13;&#10;&#9;KINAK,&#13;&#10;&#9;STUNR,&#13;&#10;&#9;'Prezzo' AS TipoCondizione,&#13;&#10;&#9;KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;KONV.KNUMV, STUNR, KPOSN, KWERT, VBRK_P.VBELN, VBRK_P.ERDAT, KOAID, KINAK&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;KNUMV, POSNR,VBRK.VBELN, VBRK.ERDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN  --AND VBRK.ERDAT = VBRP.ERDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;WHERE &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;  KNUMV = '0013873829' AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;  POSNR = '000020' AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;  FKART IN (' F2', 'FX', 'ZGEP', 'ZPDV')&#13;&#10;&#9;&#9;&#9;&#9;) VBRK_P &#13;&#10;&#9;&#9;&#9;&#9;ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;   KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B'  AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = '' ) AS K" xmi:id="_RYKY0SkHEe2lYIaDA-HbiA"/>
      <query contextMode="true" id="_A3YRoCkIEe2lYIaDA-HbiA" label="Check_FatturatoLordo_A" value="SELECT &#13;&#10;&#9;VBRK_P.ERDAT,&#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KPOSN,    &#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;&#9;'Sconto' AS TIPOCONDIZIONE,&#13;&#10;&#9;SUM(KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, POSNR, VBRK.VBELN, VBRK.ERDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN  &#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9; KNUMV = '0013873829' AND&#13;&#10;&#9;&#9; POSNR = '000020' AND&#13;&#10;&#9;&#9; FKART IN (' F2', 'FX', 'ZGEP', 'ZPDV')&#13;&#10;&#9;) &#13;&#10;&#9;&#9;VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;  &#9;&#9;&#9;&#9;  KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KOAID = 'A' &#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;&#9;VBRK_P.ERDAT,&#13;&#10;&#9;KPOSN" xmi:id="_A3YRoSkIEe2lYIaDA-HbiA"/>
