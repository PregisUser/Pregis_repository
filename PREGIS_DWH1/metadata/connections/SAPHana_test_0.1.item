      <query id="_kLzEsBMTEe2trNvDWGyjVA" label="KONV_FatturatoLordo_B" value="SELECT &#13;&#10;&#9;K.FKDAT,&#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN ,&#13;&#10;&#9;'Prezzo' AS TipoCondizione,&#13;&#10;&#9;KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;KONV.KNUMV, STUNR, KPOSN, KWERT, VBRK_P.VBELN, VBRK_P.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;KNUMV, POSNR,VBRK.VBELN, VBRK.FKDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;&#9;&#9;&#9;&#9;WHERE &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019 AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9; FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV') AND FKSTO != 'X'&#13;&#10;&#9;&#9;&#9;&#9;) VBRK_P &#13;&#10;&#9;&#9;&#9;&#9;ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;   KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B'  AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = '') K&#9;&#13;&#10;INNER JOIN &#13;&#10;&#9;(&#13;&#10;&#9;SELECT  &#13;&#10;&#9;&#9;KNUMV, KPOSN, MAX(STUNR) AS MSTUNR&#13;&#10;&#9;FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = ''&#13;&#10;&#9;GROUP BY &#13;&#10;&#9;&#9;KNUMV, KPOSN) KMAX ON K.KNUMV = KMAX.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;     &#9;&#9;&#9;&#9;&#9;&#9;&#9;        K.KPOSN = KMAX.KPOSN AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;                                    K.STUNR = KMAX.MSTUNR" xmi:id="_kLzEsRMTEe2trNvDWGyjVA"/>
      <query id="_96TWUBMrEe2trNvDWGyjVA" label="KONV_ResiCliente_B" value="SELECT &#13;&#10;&#9;K.FKDAT,&#13;&#10;&#9;K.KNUMV, &#13;&#10;&#9;K.KPOSN,&#13;&#10;&#9;K.VBELN ,&#13;&#10;&#9;'Prezzo' AS TipoCondizione,&#13;&#10;&#9;KWERT&#13;&#10;FROM &#13;&#10;&#9;(&#13;&#10;&#9;SELECT&#13;&#10;&#9;&#9;KONV.KNUMV, STUNR, KPOSN, KWERT, VBRK_P.VBELN, VBRK_P.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9; INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;(&#13;&#10;&#9;&#9;&#9;&#9;&#9;SELECT &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;KNUMV, POSNR,VBRK.VBELN, VBRK.FKDAT&#13;&#10;&#9;&#9;&#9;&#9;&#9;FROM &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRK&#13;&#10;&#9;&#9;&#9;&#9;&#9;INNER JOIN&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN &#13;&#10;&#9;&#9;&#9;&#9;&#9;WHERE &#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019 AND FKSTO != 'X' AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9; FKART IN ('RE')&#13;&#10;&#9;&#9;&#9;&#9;) VBRK_P &#13;&#10;&#9;&#9;&#9;&#9;ON KONV.KNUMV = VBRK_P.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;   KONV.KPOSN = VBRK_P.POSNR&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B'  AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = '') K&#9;&#13;&#10;INNER JOIN &#13;&#10;&#9;(&#13;&#10;&#9;SELECT  &#13;&#10;&#9;&#9;KNUMV, KPOSN, MAX(STUNR) AS MSTUNR&#13;&#10;&#9;FROM &#13;&#10;&#9;&#9;KONV&#13;&#10;&#9;WHERE &#13;&#10;&#9;&#9;KOAID = 'B' AND &#13;&#10;&#9;&#9;KINAK = '' AND&#13;&#10;&#9;&#9;KSTAT = ''&#13;&#10;&#9;GROUP BY &#13;&#10;&#9;&#9;KNUMV, KPOSN) KMAX ON K.KNUMV = KMAX.KNUMV AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;     &#9;&#9;&#9;&#9;&#9;&#9;&#9;        K.KPOSN = KMAX.KPOSN AND&#13;&#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;                                    K.STUNR = KMAX.MSTUNR" xmi:id="_96TWURMrEe2trNvDWGyjVA"/>
      <query id="_xDGWMBMyEe2trNvDWGyjVA" label="FatturatooDiverso" value="SELECT&#13;&#10;&#9;KNUMV, &#13;&#10;&#9;POSNR,&#13;&#10;    CASE WHEN FKART = 'G2' THEN VBRP.NETWR * -1 ELSE  VBRP.NETWR END AS NETWR ,&#13;&#10;FKART&#13;&#10;FROM&#13;&#10;&#9;VBRK&#13;&#10;INNER JOIN&#13;&#10;&#9;VBRP ON VBRK.VBELN = VBRP.VBELN&#13;&#10;WHERE &#13;&#10;   YEAR(VBRK.ERDAT) &gt;= 2019 AND &#13;&#10;&#9;FKART IN('L2', 'G2') " xmi:id="_xDGWMRMyEe2trNvDWGyjVA"/>
      <query id="_YV8GoBM3Ee2trNvDWGyjVA" label="KONV_ScontiVendite_A" value="SELECT &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;VBRK_P.FKDAT,&#13;&#10;&#9;'Sconto'  AS TIPOCONDIZIONE,&#13;&#10;&#9; SUM(KONV.KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;    VBRK.VBELN,&#13;&#10;VBRK.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019  AND FKSTO != 'X' AND&#13;&#10;&#9;&#9;FKART IN ('F2', 'FX', 'ZGEP', 'ZPDV')) VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KOAID = 'A'  AND&#13;&#10;&#9;KSTAT = '' AND&#13;&#10; &#9;KINAK = '' AND  KSCHL != 'ZGEP'&#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;&#9;VBRK_P.FKDAT" xmi:id="_YV8GoRM3Ee2trNvDWGyjVA"/>
      <query id="_5rt_kBM3Ee2trNvDWGyjVA" label="KONV_ScontiResi_A" value="SELECT &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;     &#9;VBRK_P.FKDAT,&#13;&#10;&#9;'Sconto'  AS TIPOCONDIZIONE,&#13;&#10;&#9; SUM(KONV.KWERT) AS KWERT&#13;&#10;FROM &#13;&#10;&#9;KONV&#13;&#10;INNER JOIN&#13;&#10;&#9;(&#13;&#10;&#9; SELECT &#13;&#10;&#9;&#9;KNUMV, &#13;&#10;&#9;&#9;POSNR,&#13;&#10;&#9;&#9;VBRK.VBELN,&#13;&#10;&#9;&#9;VBRK.FKDAT&#13;&#10;&#9; FROM &#13;&#10;&#9;&#9;VBRK&#13;&#10;&#9; INNER JOIN&#9;&#13;&#10;&#9;&#9;VBRP ON&#9;VBRK.VBELN = VBRP.VBELN&#13;&#10;&#9; WHERE &#13;&#10;&#9;&#9;YEAR(VBRK.FKDAT) &gt;= 2019 AND FKSTO != 'X' AND &#13;&#10;&#9;&#9;FKART = 'RE') VBRK_P ON KONV.KNUMV = VBRK_P.KNUMV AND KONV.KPOSN = VBRK_P.POSNR&#13;&#10;WHERE &#13;&#10;&#9;KOAID = 'A'  AND&#13;&#10;&#9;KSTAT = '' and&#13;&#10; &#9;KINAK = ''&#13;&#10;GROUP BY &#13;&#10;&#9;KONV.KNUMV,&#13;&#10;&#9;KONV.KPOSN,&#13;&#10;&#9;VBRK_P.VBELN,&#13;&#10;     VBRK_P.FKDAT" xmi:id="_5rt_kRM3Ee2trNvDWGyjVA"/>
